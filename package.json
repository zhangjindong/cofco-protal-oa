    "build:prod": "ng build --prod --aot --sourceMap=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "hmr": "ng serve --proxy-config proxy.json --hmr -c=hmr --host portal.baoli.com"
